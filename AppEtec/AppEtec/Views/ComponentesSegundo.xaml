<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppEtec.Views.ComponentesSegundo">
    <ContentPage.Content>

        <ScrollView>
            <StackLayout VerticalOptions="CenterAndExpand" Padding="10">
                <Button Clicked="btnIngles" Text="Inglês Instrumental" Style="{ StaticResource btn_cinza }" />
                <Button Clicked="btnDs" Text="Desenvolvimento de Sistemas" Style="{ StaticResource btn_cinza }" />
                <Button Clicked="btnBancoII" Text="Banco de Dados II" Style="{ StaticResource btn_cinza }" />
                <Button Clicked="btnInternet" Text="Internet e Protocolos" Style="{ StaticResource btn_cinza }" />
                <Button Clicked="btnMobile" Text="Programação de Aplicativos Mobile I" Style="{ StaticResource btn_cinza }" />
                <Button Clicked="btnWebII" Text="Programação Web II" Style="{ StaticResource btn_cinza }" />
                <Button Clicked="btnSE" Text="Sistemas Embarcados" Style="{ StaticResource btn_cinza }" />
            </StackLayout>
        </ScrollView>
        
    </ContentPage.Content>
</ContentPage>